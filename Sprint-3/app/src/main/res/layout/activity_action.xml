<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.qwerty.acer.facebook52.Action">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="75dp"
        android:background="#1d43be">

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton6"
            android:background="@drawable/homee"
            android:layout_alignParentBottom="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton7"
            android:background="@drawable/frnds"
            android:layout_alignParentBottom="true"
            android:layout_toRightOf="@+id/imageButton6"
            android:layout_toEndOf="@+id/imageButton6" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:background="@drawable/babbu"
            android:id="@+id/imageButton8"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageButton9"
            android:layout_toStartOf="@+id/imageButton9" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:id="@+id/imageButton9"
            android:background="@drawable/not"
            android:layout_alignParentBottom="true"
            android:layout_toLeftOf="@+id/imageButton10"
            android:layout_toStartOf="@+id/imageButton10" />

        <ImageButton
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/zoom"
            android:id="@+id/imageButton10"
            android:layout_alignTop="@+id/imageButton9"
            android:layout_toLeftOf="@+id/imageButton11"
            android:layout_toStartOf="@+id/imageButton11" />

        <ImageButton
            android:layout_width="65dp"
            android:layout_height="60dp"
            android:background="@drawable/multiple_choice1600"
            android:id="@+id/imageButton11"
            android:layout_alignTop="@+id/imageButton10"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true" />
    </RelativeLayout>
</RelativeLayout>
